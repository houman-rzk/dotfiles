[binds]
j = next
#l = next
#n = next
k = prev
#p = prev
#h = prev
q = quit
<plus> = zoom 1
<minus> = zoom -1
r = rotate by 90
f = fullscreen
o = overlay
gg = goto 1
<Shift+G> = goto -1
p = next_frame
<space> = toggle_playing
