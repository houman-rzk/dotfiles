#/bin/sh

xrandr --output Virtual-1 --mode 1920x1080
#xrandr --output Virtual-1 --mode 1024x768
#xrandr --output Virtual-1 --mode 1600x900

#xrdb /home/ache/.config/x11/xresources & xrdbpid=$!

setxkbmap -layout es

autostart="xcompmgr" #xrdb"
#autostart="picom"

for prog in $autostart; do
	pidof -s "$prog" || "$prog" &
done >/dev/null 2>&1

setbg


#[ -n "$xrdbpid" ] && wait "$xrdbpid"
